# Program python yang mencoba menjelaskan
# pemakaian filter
	
# fungsi memfilter huruf vokal
def fun(variable):
	
	letters = ['a', 'e', 'i', 'o', 'u']
	
	if (variable in letters):
		return True
	else:
		return False
	
	
# sequence
sequence = ['g', 'e', 'e', 'j', 'k', 's', 'p', 'r']
	
# memakai fungsi filter
filtered = filter(fun, sequence)
	
print('huruf yang difilter adalah :')

for s in filtered:
	print(s)
